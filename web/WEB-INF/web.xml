<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <servlet>
        <servlet-name>RDS</servlet-name>
        <servlet-class>org.servlet.RequestDemoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RDS</servlet-name>
        <url-pattern>/rdservlet</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>RFS</servlet-name>
        <servlet-class>org.servlet.RefererServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>RFS</servlet-name>
        <url-pattern>/rfservlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FS</servlet-name>
        <servlet-class>org.servlet.FormServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FS</servlet-name>
        <url-pattern>/submit</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CS</servlet-name>
        <servlet-class>org.servlet.CookieServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CS</servlet-name>
        <url-pattern>/cookie</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>LBS</servlet-name>
        <servlet-class>org.cart.ListBookServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>LBS</servlet-name>
        <url-pattern>/lbservlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PCS</servlet-name>
        <servlet-class>org.cart.PurchaseServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PCS</servlet-name>
        <url-pattern>/pcservlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CTS</servlet-name>
        <servlet-class>org.cart.CartServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CTS</servlet-name>
        <url-pattern>/ctservlet</url-pattern>
    </servlet-mapping>
</web-app>